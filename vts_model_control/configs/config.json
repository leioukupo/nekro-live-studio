{
    "plugin": {
        "plugin_name": "vts模型控制插件",
        "plugin_developer": "Zaxpris",
        "default_vts_endpoint": "ws://localhost:8002",
        "debug_mode": false,
        "restore_duration": 3.0,
        "cleanup_timeout": 5.0
    },
    "blink": {
        "enabled": true,
        "min_interval": 2.0,
        "max_interval": 4.0,
        "close_duration": 0.15,
        "open_duration": 0.3,
        "closed_hold": 0.05,
        "left_parameter": "EyeOpenLeft",
        "right_parameter": "EyeOpenRight",
        "min_value": 0.0,
        "max_value": 1.0
    },
    "breathing": {
        "enabled": true,
        "min_value": -3.0,
        "max_value": 3.0,
        "inhale_duration": 1.0,
        "exhale_duration": 2.0,
        "parameter": "FaceAngleY"
    },
    "body_swing": {
        "enabled": true,
        "x_min": -10.0,
        "x_max": 15.0,
        "z_min": -10.0,
        "z_max": 15.0,
        "min_duration": 2.0,
        "max_duration": 8.0,
        "x_parameter": "FaceAngleX",
        "z_parameter": "FaceAngleZ"
    },
    "eye_follow": {
        "enabled": true,
        "x_min_range": -1.0,
        "x_max_range": 1.0,
        "y_min_range": -1.0,
        "y_max_range": 1.0,
        "left_x_parameter": "EyeLeftX",
        "right_x_parameter": "EyeRightX",
        "left_y_parameter": "EyeLeftY",
        "right_y_parameter": "EyeRightY"
    },
    "mouth_expression": {
        "enabled": true,
        "smile_min": 0.1,
        "smile_max": 0.7,
        "open_min": 0.1,
        "open_max": 0.7,
        "change_min_duration": 2.0,
        "change_max_duration": 7.0,
        "smile_parameter": "MouthSmile",
        "open_parameter": "MouthOpen"
    },
    "models": [],
    "api": {
        "enabled": true,
        "host": "0.0.0.0",
        "port": 8080,
        "timeout": 5.0
    }
}